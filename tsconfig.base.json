{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "ES2020",
    "moduleResolution": "node",
    "esModuleInterop" :true,
    "sourceMap": true,
    "declaration": true,
    "strict": true,
    "skipLibCheck": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "incremental": true,
    "removeComments": true,
    "forceConsistentCasingInFileNames": false,
    "resolveJsonModule": true
  },
}
